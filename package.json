    "esquery": "^1.3.1",
    "loader-utils": "^1.4.0",
    "schema-utils": "^2.7.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
